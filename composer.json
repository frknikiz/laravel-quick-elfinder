{
    "name": "frknikiz/laravel-quick-elfinder",
    "description": "Laravel Quick Elfinder package allows you to use Elfinder quickly.",
    "keywords": ["laravel","elfinder", "quick","package","laravel4.2"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Furkan Ikiz",
            "email": "frknikiz@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "barryvdh/laravel-elfinder": "0.2.*"
    },
    "autoload": {
        "classmap": [
            "src/config",
            "src/views"
        ],
        "psr-0": {
            "Frknikiz\\CustomElfinder\\": "src/"
        }
    },
    "minimum-stability": "stable"
